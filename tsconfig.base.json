{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2018",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "typeRoots": ["./node_modules/@types"],
    "allowJs": false,
    "skipLibCheck": false,
    "downlevelIteration": true,
    "paths": {}
  }
}
